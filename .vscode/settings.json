{
    "vue-format.html_indent_root": true,
    "vue-format.js-beautify": {
      "indent_size": "editor.tabSize",
      "indent_char": " ",
      "indent_with_tabs": false,
      "brace-style": "collapse-preserve-inline",
      "space_after_anon_function": true,
      "css": {},
      "js": {},
      "html": {
        "force_format": [
          "template"
        ]
      }
    },
    "vue-format.pug-beautify": {
      "fill_tab": false,
      "tabSize": 2,
  },
  "beautify.config": {
    "indent_size": "editor.tabSize",
    "indent_char": " ",
    "indent_with_tabs": false,
    "brace-style": "collapse-preserve-inline",
    "space_after_anon_function": true,
    "css": {},
    "js": {},
    "html": {
      "force_format": [
        "template"
      ]
    }
  }
}
